#include stdlib/funcs.brick

brick main{
    dec x as strbuf -> 32;
    dec y as string -> "Hello World"; //basically just a string constant

    set x to "Works?";
    output x;
    set x to y;
    output y;

    dec str as array byte -> 32 {"Hey, what's up?: "};

    print_string(&str);

    input str;

    print_string(&str);
    output;


    exit 0;
}