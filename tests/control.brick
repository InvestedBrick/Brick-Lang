brick main{
    dec x as int -> 5;

    if x < 11{
        dec n as short -> 0;

        while x > 0{
            x--;
            n++;
        }

        for dec i as int -> 0; i < x + 5 and i != 3; i++;{
            n--;
        }
        set x to n;

    }else{
        exit 3;
    }

    dec n as int -> 0;
    while true == true {

        for dec i as int -> 0; i < 5; i++; {
            x--;
            if x > 0{
                next;
            }
            n++;
            if x == 0 {
                break;
            }
        }
        if x == 0{
            break;
        }
    }

    exit x + n;
}