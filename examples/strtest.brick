brick main{
    dec x as strbuf -> 32;
    dec y as string -> "Hello World"; //basically just a string constant

    set x to "Works?";
    output x;
    set x to y;
    output y;

    dec str as array byte -> 32 {"Hey, what's up?: "};

    dec i as int -> 0;
    while str[i] != null {
        output str[i++], noend;
    }

    input str;

    set i to 0;
    while str[i] != null {
        output str[i++], noend;
    }
    output;


    exit 0;
}