#include stdlib/int_to_ascii.brick

brick return_five{
    return 5;
}=>int

brick return_three{
    return 3;
}=>int

brick main{
    
    dec x as int -> 0;
    for dec i as int -> 0; i < 10 and x < 5; i++;{
        output itoa(i), " " noend;
        x++;
    }
    output;
    //output itoa(x);
    //output itoa(x++);
    //output itoa(x--);
    dec y as int -> 4;
    dec var as int -> (return_five() + return_three() * 5) / y;
    set var to var | 2; // 0b101 | 0b010 == 111 = 7
    set var to var ^ 3; // 0b111 ^ 0b011 == 100 = 4
    output itoa(var);

    exit 0;
}